AddDefaultCharset UTF-8

SetEnv TZ America/New_York

<IfModule mod_php5.c>
	php_value memory_limit -1
</IfModule>

<IfModule mod_rewrite.c>
  RewriteEngine on
  RewriteBase /
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^(.*)$ index.php/$1 [L,QSA]
</IfModule>
